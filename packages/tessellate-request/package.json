{
  "name": "tessellate-request",
  "version": "1.1.1",
  "description": "Tessellate request library.",
  "author": "Sebastian Monte <sebastian.monte@zalando.de>",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "tag": "next"
  },
  "engines": {
    "node": ">=v7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/tessellate.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "babel -d dist lib",
    "lint": "eslint lib test",
    "test": "npm run lint && NODE_ENV=test jest --coverage",
    "flow-typed-install": "rimraf flow-typed/npm && flow-typed install -o",
    "prepublishOnly": "npm run clean && npm run dist",
    "upgrade-dependencies": "ncu -u"
  },
  "dependencies": {
    "client-oauth2": "3.4.1",
    "nconf": "0.8.4",
    "node-tokens": "0.0.10",
    "request": "2.80.0",
    "request-promise-native": "1.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "eslint": "3.17.1",
    "flow-bin": "0.41.0",
    "flow-typed": "2.0.0",
    "jest": "19.0.2",
    "npm-check-updates": "2.10.3",
    "rimraf": "2.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  }
}
